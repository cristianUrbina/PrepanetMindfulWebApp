extends layout

block content
  h1 Rango de fechas para inscripci√≥n
  form(action="/registrar-fechas" method="POST" enctype="multipart/form-data")
    label(for="date-begin") Fecha de inicio
    input(type="date" name="begin" id="date-begin" required)
    label(for="date-end") Fecha de fin
    input(type="date" name="end" id="date-end" required)
    input(type="submit" value="Registrar")
  if (dates) 
    p Fecha de inicio #{dates.begin.date}
    p Fecha de fin #{dates.end.date}
